#!/bin/sh

OSX=/Applications/Emacs.app/Contents/MacOS/Emacs
LOCAL=$HOME/tools/emacs-26.1-install/bin/emacs
PATH=$PATH:$HOME/tools/ccls/Release
PATH=$PATH:$HOME/tools/clang+llvm-9.0.0-x86_64-linux-gnu-ubuntu-18.04/bin

if [ -f $OSX ]; then
  $OSX $@
else
  if [ -f $LOCAL ]; then
    $LOCAL $@
  else
    if [ -f /usr/bin/emacs ]; then
      /usr/bin/emacs $@
    fi
  fi
fi
